{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://openclaw.local/schemas/economic_run.schema.json",
  "title": "economic_run",
  "type": "object",
  "required": [
    "run_id",
    "benchmark",
    "scenario",
    "success",
    "quality_score",
    "total_cost_usd",
    "provider_path",
    "started_at",
    "ended_at"
  ],
  "properties": {
    "run_id": {
      "type": "string",
      "minLength": 1
    },
    "benchmark": {
      "type": "string",
      "minLength": 1
    },
    "scenario": {
      "type": "string",
      "minLength": 1
    },
    "success": {
      "type": "boolean"
    },
    "quality_score": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    },
    "total_cost_usd": {
      "type": "number",
      "minimum": 0
    },
    "provider_path": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "started_at": {
      "type": "string",
      "format": "date-time"
    },
    "ended_at": {
      "type": "string",
      "format": "date-time"
    },
    "notes": {
      "type": "string"
    },
    "metadata": {
      "type": "object",
      "additionalProperties": true
    }
  },
  "additionalProperties": false
}
